<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="default">

        <class>sv.edu.udb.www.entities.AlquilerEntity</class>
        <class>sv.edu.udb.www.entities.FavoritosEntity</class>
        <class>sv.edu.udb.www.entities.ImgEntity</class>
        <class>sv.edu.udb.www.entities.OfertaEntity</class>
        <class>sv.edu.udb.www.entities.TipousuarioEntity</class>
        <class>sv.edu.udb.www.entities.UsuarioEntity</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/airbnb?useSSL=false"/>
            <property name="eclipselink.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
            <property name="eclipselink.jdbc.user" value="root"/>
        </properties>
    </persistence-unit>
</persistence>
